{
  "name": "sommark",
  "displayName": "SomMark",
  "description": "Syntax highlighting and basic language support for SomMark, a minimal, extensible markup language for documentation.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "sommark",
      "aliases": ["SomMark", "sommark"],
      "extensions": [".smark"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "sommark",
      "scopeName": "source.sommark",
      "path": "./syntaxes/sommark.tmLanguage.json"
    }]
  }
}
